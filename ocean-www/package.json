  "main": "index.js",
  "scripts": {
    "build": "npm install && set NODE_ENV=production && ts-node scripts/build.ts",
    "devServer": "ts-node scripts/devServer.ts",
    "devClient": "ts-node scripts/devClient.ts",
    "devServerAndClient": "concurrently --kill-others \"npm run devServer\" \"npm run devClient\"",
    "dev": "set NODE_ENV=development && npm install && npm run devServerAndClient",
    "prod": "set NODE_ENV=production && npm install && npm run devServerAndClient",
    "debug": "set DEBUG=express:* && npm run dev",
    "start": "node index.js",
    "buildAndDeploy": "npm run build && cd dist && gcloud app deploy",
    "getData": "node scripts/getData.js",
    "getUsers": "node scripts/getUsers.js"
  },
